    "openapi-typescript": "6.4.0",
    "postcss": "8.4.28",
    "postcss-modules": "6.0.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-router-dom": "6.15.0",
    "semantic-ui-react": "2.1.4"
  },
  "devDependencies": {
    "@tanstack/eslint-plugin-query": "4.34.1",
