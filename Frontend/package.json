    "url": "https://github.com/thewca/wca-registration/issues"
  },
  "homepage": "https://github.com/thewca/wca-registration/Frontend/README.md",
  "scripts": {
    "build:types": "TYPES_ONLY=true node src/build.mjs",
    "build:validations": "openapi-zod ../swagger/v1/swagger.yaml --output ./src/api/validations.ts",
    "build:dev": "NODE_ENV=\"development\" ASSET_PATH=\"/dist\" API_URL=\"http://localhost:3001\" AUTH_URL=\"http://localhost:3001/test/jwt\" node src/build.mjs",
    "build:staging": "POLL_URL=\"https://1rq8d7dif3.execute-api.us-west-2.amazonaws.com/v1/staging\" NODE_ENV=\"production\" ASSET_PATH=\"https://d1qizdh27al0a7.cloudfront.net/staging/dist\" API_URL=\"https://staging.registration.worldcubeassociation.org\" WCA_URL=\"https://staging.worldcubeassociation.org\" node src/build.mjs",
    "build:prod": "POLL_URL=\"https://1rq8d7dif3.execute-api.us-west-2.amazonaws.com/v1/prod\" NODE_ENV=\"production\" ASSET_PATH=\"https://d1qizdh27al0a7.cloudfront.net/dist\" API_URL=\"https://registration.worldcubeassociation.org\" WCA_URL=\"https://worldcubeassociation.org\" node src/build.mjs",
    "watch": "node src/watch.mjs",
    "lint": "eslint src --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "eslint src --ext .js,.jsx,.ts,.tsx --fix",
    "test:contract": "npm run build:validations && jest"
  },
  "dependencies": {
    "@dinero.js/currencies": "2.0.0-alpha.14",
    "@fullcalendar/luxon3": "6.1.11",
    "@fullcalendar/react": "6.1.11",
    "@fullcalendar/timegrid": "6.1.11",
    "@stripe/react-stripe-js": "2.6.2",
    "@stripe/stripe-js": "3.0.7",
    "@tanstack/react-query": "4.33.0",
    "@tanstack/react-query-devtools": "4.33.0",
    "@thewca/wca-components": "0.6.0",
    "@wca/helpers": "1.1.5",
    "dinero.js": "2.0.0-alpha.14",
    "events": "3.3.0",
    "fomantic-ui-css": "2.9.3",
    "jest-localstorage-mock": "2.4.26",
    "i18next": "23.10.0",
    "luxon": "3.4.4",
    "marked": "12.0.1",
    "openapi-fetch": "0.9.2",
    "openapi-typescript": "6.7.5",
    "postcss": "8.4.35",
    "postcss-modules": "6.0.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-i18next": "14.1.0",
    "react-router-dom": "6.22.2",
    "semantic-ui-react": "2.1.5"
  },
  "devDependencies": {
    "@jest/globals": "29.7.0",
    "@tanstack/eslint-plugin-query": "5.20.1",
    "@types/luxon": "3.4.2",
    "@types/node": "20.11.24",
    "esbuild": "0.20.2",
    "esbuild-sass-plugin": "3.1.0",
    "eslint": "8.57.0",
    "eslint-kit": "10.24.0",
    "fishery": "2.2.2",
    "jest": "29.7.0",
    "openapi-zod": "0.2.7",
