{
  "name": "wca-registration-frontend",
  "version": "1.0.0",
  "description": "The Frontend for the WCA Registration service",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/thewca/wca-registration.git"
  },
  "author": "WCA Software Team",
